package br.edu.principal;
public class principal {

	public static void main(String[] args) {
double Saco,Racao_do_gato1, Racao_do_gato2, Total ;
		
		Saco = 10; 
		Racao_do_gato1 = 100; 
		Racao_do_gato2 = 100; 
		
		Racao_do_gato1 = Racao_do_gato1 / 1000;
		Racao_do_gato2 = Racao_do_gato2 / 1000;
		
		Total = Saco - 5 *(Racao_do_gato1 + Racao_do_gato2);
		System.out.println("Depois de passar 5 dias o saco vai ter : " + Total + " kg");
		

	}

}
