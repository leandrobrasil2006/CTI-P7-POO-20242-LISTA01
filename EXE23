package br.edu.principal;
import java.util.Scanner;
public class principal {

	public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
	     
        double Numero,I, F;
        long A;
        
        System.out.print("Insira algum número real: ");
        Numero = scanner.nextDouble();
      
       I = Math.floor(Numero); 
        F = Numero - I;
        A = Math.round(Numero);
       
        System.out.printf("Parte inteira do numero: %.0f%n", I);
        System.out.printf("Parte fracionária do numero: %.2f%n", F);
        System.out.printf("Número arredondado do numero: %d%n", A);
        

        scanner.close();

	}

}
